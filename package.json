{
  "name": "babble",
  "displayName": "Babble",
  "description": "Language support for Babble",
  "publisher": "aaronc81",
  "author": {
    "name": "Aaron Christiansen",
    "email": "aaronc20000@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AaronC81/babble-vscode"
  },
  "icon": "img/icon.png",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.73.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "babble",
      "aliases": ["Babble", "babble"],
